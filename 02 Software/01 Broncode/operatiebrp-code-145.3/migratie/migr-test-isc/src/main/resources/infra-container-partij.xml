<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="partijRegisterService" class="nl.bzk.migratiebrp.test.isc.environment.kanaal.jms.RegisterHandler"/>

    <bean id="partijRegisterListener" class="org.springframework.jms.listener.DefaultMessageListenerContainer">
        <property name="autoStartup" value="false"/>
        <property name="pubSubDomain" value="true"/>
        <property name="sessionTransacted" value="true"/>

        <property name="connectionFactory" ref="iscConnectionFactory"/>
        <property name="destination" ref="partijRegister"/>

        <property name="messageListener" ref="partijRegisterService"/>
    </bean>

</beans>
