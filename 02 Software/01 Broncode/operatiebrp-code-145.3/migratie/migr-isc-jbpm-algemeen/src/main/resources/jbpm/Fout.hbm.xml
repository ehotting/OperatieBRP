<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field" >
  <class name="nl.bzk.migratiebrp.isc.runtime.jbpm.model.Fout" table="MIG_FOUT" >

    <id name="id" column="ID"><generator class="native" /></id>

	<property name="tijdstip" column="TIJDSTIP" />
	<property name="proces" column="PROCES" />

    <many-to-one name="processInstance"
                 column="PROCESS_INSTANCE_ID"
                 foreign-key="PGD_PIE_FK1"
                 index="PGD_IX1"/>

    <property name="procesInitGemeente" column="PROCES_INIT_GEMEENTE" />
    <property name="procesDoelPartijCode" column="PROCES_DOEL_GEMEENTE"/>

    <property name="code" column="CODE"/>
    <property name="melding" column="MELDING"/>
    <property name="resolutie" column="RESOLUTIE"/>

  </class>
</hibernate-mapping>
