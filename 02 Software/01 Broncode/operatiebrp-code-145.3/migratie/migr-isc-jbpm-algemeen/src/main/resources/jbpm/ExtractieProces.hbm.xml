<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field" >
  <class name="nl.bzk.migratiebrp.isc.runtime.jbpm.model.ExtractieProces" table="MIG_EXTRACTIE_PROCES" >

	<id name="processInstanceId" type="java.lang.Long">
		<column name="PROCESS_INSTANCE_ID" />
		<generator class="foreign">
			<param name="property">processInstance</param>
		</generator>
	</id>
	<one-to-one name="processInstance" ></one-to-one>

	<property name="procesNaam" column="PROCES_NAAM" />
	<property name="berichtType" column="BERICHT_TYPE" />
	<property name="kanaal" column="KANAAL" />
	<property name="foutreden" column="FOUTREDEN" />
	<property name="anummer" column="ANUMMER" />
	<property name="startdatum" column="STARTDATUM" />
	<property name="einddatum" column="EINDDATUM" />
	<property name="wachtStartdatum" column="WACHT_STARTDATUM" />
	<property name="wachtEinddatum" column="WACHT_EINDDATUM" />

  </class>
</hibernate-mapping>
