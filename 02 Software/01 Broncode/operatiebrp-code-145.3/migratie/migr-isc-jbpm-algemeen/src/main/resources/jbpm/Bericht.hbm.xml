<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field">
  <class name="nl.bzk.migratiebrp.isc.runtime.jbpm.model.Bericht" table="MIG_BERICHT">

    <id name="id" column="ID"><generator class="native" /></id>

    <property name="tijdstip" column="TIJDSTIP" />
    <property name="kanaal" column="KANAAL" />
    <property name="richting" column="RICHTING" />
    <property name="messageId" column="MESSAGE_ID" />
    <property name="correlationId" column="CORRELATION_ID" />
    <property name="bericht" column="BERICHT" />
    <property name="naam" column="NAAM" />

    <many-to-one name="processInstance"
                 column="PROCESS_INSTANCE_ID"
                 foreign-key="BRT_PIE_FK1"
                 index="BRT_IX1"/>

 <many-to-one name="virtueelProces"
                 column="VIRTUEEL_PROCES_ID"
                 foreign-key="BRT_VPS_FK1"
                 index="BRT_IX2"/>

    <property name="verzendendePartij" column="VERZENDENDE_PARTIJ" />
    <property name="ontvangendePartij" column="ONTVANGENDE_PARTIJ" />
    <property name="msSequenceNumber" column="MS_SEQUENCE_NUMBER" />
    <property name="requestNonReceipt" column="REQUEST_NON_RECEIPT" />
    <property name="actie" column="ACTIE" />
    <property name="indicatieGeteld" column="INDICATIE_GETELD" />
  </class>
</hibernate-mapping>
