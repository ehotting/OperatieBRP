<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field">
  <class name="nl.bzk.migratiebrp.isc.runtime.jbpm.model.TaakGerelateerd" table="MIG_TAAK_GERELATEERD" >

    <id name="id" column="ID"><generator class="native" /></id>

    <many-to-one name="taskInstance"
                 column="TASK_INSTANCE_ID"
                 foreign-key="TKG_TSK_FK1"
                 index="TKG_IX1"/>

    <property name="administratienummer" column="ADMINISTRATIENUMMER"/>

  </class>
</hibernate-mapping>
