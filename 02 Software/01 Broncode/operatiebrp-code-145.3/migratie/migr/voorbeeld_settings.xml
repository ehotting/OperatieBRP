<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<settings>
	<activeProfiles>
		<activeProfile>mgba</activeProfile>
	</activeProfiles>
	<servers>
		<server>
			<id>libs-release-migratie</id>
			<username>user</username>
			<password>{encrypted wachtwoord}</password>
		</server>
		<server>
			<id>libs-snapshot-migratie</id>
			<username>user</username>
			<password>{encrypted wachtwoord}</password>
		</server>
		<server>
			<id>local-mgba</id>
			<username>user</username>
			<password>{encrypted wachtwoord}</password>
		</server>
	</servers>
	<profiles>
		<profile>
			<id>mgba</id>
			<repositories>
				<repository>
					<id>local-mgba</id>
					<name>mgba-repos</name>
					<url>https://www.modernodam.nl/artifactory/mgba-repo</url>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</snapshots>
				</repository>
				<repository>
					<id>libs-release-migratie</id>
					<name>mGBA Artifactory-releases voor migratie</name>
					<url>https://www.modernodam.nl/artifactory/libs-release-migratie
					</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
				</repository>
				<repository>
					<id>libs-snapshot-migratie</id>
					<name>mGBA Artifactory-snapshots voor migratie</name>
					<url>https://www.modernodam.nl/artifactory/libs-snapshot-migratie
					</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>artifactory</id>
					<name>mgba-repos</name>
					<url>https://www.modernodam.nl/artifactory/mgba-repo</url>
					<layout>default</layout>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
				</pluginRepository>
				<pluginRepository>
					<id>libs-release-migratie</id>
					<name>mGBA Artifactory-releases voor migratie</name>
					<url>https://www.modernodam.nl/artifactory/libs-release-migratie
					</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
				</pluginRepository>
				<pluginRepository>
					<id>libs-snapshot-migratie</id>
					<name>mGBA Artifactory-snapshots voor migratie</name>
					<url>https://www.modernodam.nl/artifactory/libs-snapshot-migratie
					</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
</settings>