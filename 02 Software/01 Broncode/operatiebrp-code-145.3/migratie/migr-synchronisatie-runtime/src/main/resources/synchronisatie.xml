<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:property-placeholder/>

    <import resource="classpath:alg-util-common-logging.xml"/>
    <import resource="synchronisatie-dal.xml"/>

    <import resource="synchronisatie-datasource.xml"/>
    <import resource="synchronisatie-jta.xml"/>

    <import resource="synchronisatie-jms-isc.xml"/>
    <import resource="synchronisatie-jms-brp.xml"/>

    <import resource="synchronisatie-listeners.xml"/>
    <import resource="synchronisatie-handler.xml"/>
    <import resource="synchronisatie-listeners-brp.xml"/>
    <import resource="synchronisatie-listeners-register.xml"/>

    <import resource="classpath:algemeen-services-objectsleutel-beans.xml"/>
    <import resource="classpath:vragen-beans.xml"/>

    <bean id="syncParameters" class="nl.bzk.migratiebrp.synchronisatie.dal.service.SyncParameters">
        <property name="initieleVulling" value="false"/>
    </bean>

    <import resource="synchronisatie-jmx.xml"/>
</beans>
