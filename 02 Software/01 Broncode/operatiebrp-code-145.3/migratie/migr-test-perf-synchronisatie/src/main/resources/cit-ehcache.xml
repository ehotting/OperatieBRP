<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="voiscCache" updateCheck="false">
    <defaultCache maxElementsInMemory="0" eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0" overflowToDisk="false"/>

    <!-- Aantal mailboxen: 5 centrale mailboxen; 400-500 gemeente mailboxen: 2000-3000 afnemers -->
    <cache name="mailboxenOpInstantiecode" maxElementsInMemory="5000" eternal="false" timeToLiveSeconds="86400"/>

    <!-- Aantal mailboxen: 5 centrale mailboxen; 400-500 gemeente mailboxen: 2000-3000 afnemers -->
    <cache name="mailboxenOpMailboxnr" maxElementsInMemory="5000" eternal="false" timeToLiveSeconds="86400"/>

    <!-- 1 partijregister -->
    <cache name="partijregister" maxElementsInMemory="1" eternal="false" timeToLiveSeconds="3600"/>
</ehcache>
