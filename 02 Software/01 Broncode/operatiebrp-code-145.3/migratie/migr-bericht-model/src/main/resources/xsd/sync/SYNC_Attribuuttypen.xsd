    <simpleType name="AdHocZoekAntwoordStatus">
        <restriction base="string">
            <enumeration value="A">
                <annotation>
                    <documentation>
                        PL is Actueel
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="E">
                <annotation>
                    <documentation>
                        bijhouden PL opgeschort wegens emigratie m.i.v. "datum"
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="M">
                <annotation>
                    <documentation>
                        bijhouden PL opgeschort wegens Ministerieel besluit m.i.v. "datum"
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="O">
                <annotation>
                    <documentation>
                        bijhouden PL opgeschort wegens overlijden op "datum"
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="R">
                <annotation>
                    <documentation>
                        bijhouden PL opgeschort wegens aanleg in de RNI op "datum"
                    </documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>

    <simpleType name="AdHocZoekAntwoordFoutReden">
        <restriction base="string">
            <enumeration value="B">
                <annotation>
                    <documentation>
                        PL is geblokkeerd i.v.m. verhuizing naar gemeente "code"
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="G">
                <annotation>
                    <documentation>
                        adres dan wel persoon komt niet voor
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="H">
                <annotation>
                    <documentation>
                        betrokkene heeft om geheimhouding verzocht
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="P">
                <annotation>
                    <documentation>
                        antwoordbericht te groot
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="R">
                <annotation>
                    <documentation>
                        geen autorisatie voor het stellen van ad hoc vragen over deze persoon
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="U">
                <annotation>
                    <documentation>
                        eenduidige identificatie niet gelukt
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="V">
                <annotation>
                    <documentation>
                        persoon is verhuisd naar gemeente "code"
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="X">
                <annotation>
                    <documentation>
                        geen autorisatie voor de gevraagde actie
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="Z">
                <annotation>
                    <documentation>
                        autorisatie voor geen van de personen op het aangegeven adres (bij
                        adresidentificatie) dan wel geen autorisatie voor de gezochte persoon (bij
                        persoonsidentificatie)
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="F">
                <annotation>
                    <documentation>
                        geeft aan dat er een technische fout is opgetreden in de service. F is geen
                        Lo3 foutreden en wordt vertaald naar een Pf03
                    </documentation>
                </annotation>
            </enumeration>
        </restriction>
