<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="gbavContext" lazy-init="true"
        class="nl.gba.gbav.impl.util.configuration.DeploymentContextSpringImpl">
        <property name="name">
            <value>unit-test</value>
        </property>
        <property name="contextPath">
            <value>
                classpath:gbavconfig/coreContext.xml,
                classpath:gbavconfig/bcmContext.xml,
                classpath:gbavconfig/bcmContextProperties.xml
            </value>
        </property>
    </bean>
</beans>
