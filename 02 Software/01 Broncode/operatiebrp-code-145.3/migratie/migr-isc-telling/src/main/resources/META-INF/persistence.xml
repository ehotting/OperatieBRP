<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
    xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
    <persistence-unit name="TellingEntities" transaction-type="JTA">
        <provider>org.hibernate.jpa.HibernatePersistence</provider>
        <jta-data-source>tellingDataSource</jta-data-source>

        <class>nl.bzk.migratiebrp.isc.telling.entiteit.ProcesTelling</class>
        <class>nl.bzk.migratiebrp.isc.telling.entiteit.ProcesExtractie</class>
        <class>nl.bzk.migratiebrp.isc.telling.entiteit.Bericht</class>
        <class>nl.bzk.migratiebrp.isc.telling.entiteit.BerichtTelling</class>
        <class>nl.bzk.migratiebrp.isc.telling.entiteit.ProcesInstantie</class>
        <class>nl.bzk.migratiebrp.isc.telling.entiteit.ProcesVirtueel</class>
        <class>nl.bzk.migratiebrp.isc.telling.entiteit.Runtime</class>

        <shared-cache-mode>ENABLE_SELECTIVE</shared-cache-mode>
    </persistence-unit>
</persistence>
