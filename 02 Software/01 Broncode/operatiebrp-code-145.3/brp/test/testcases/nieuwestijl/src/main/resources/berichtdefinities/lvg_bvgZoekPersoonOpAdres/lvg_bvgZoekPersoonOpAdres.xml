<brp:lvg_bvgZoekPersoonOpAdres xmlns:brp="http://www.bzk.nl/brp/brp0200">
    <brp:stuurgegevens brp:communicatieID="${stuurgegevens.communicatieID}">
        <brp:zendendePartij>${stuurgegevens.zendendePartij}</brp:zendendePartij>
        <brp:zendendeSysteem>${stuurgegevens.zendendeSysteem}</brp:zendendeSysteem>
        <brp:referentienummer>${stuurgegevens.referentienummer}</brp:referentienummer>
        <brp:tijdstipVerzending>${stuurgegevens.tijdstipVerzending}</brp:tijdstipVerzending>
    </brp:stuurgegevens>
    <brp:parameters brp:communicatieID="${parameters.communicatieID}">
        <brp:rolNaam>${parameters.rolNaam}</brp:rolNaam>
        <brp:leveringsautorisatieIdentificatie>${parameters.leveringsautorisatieIdentificatie}</brp:leveringsautorisatieIdentificatie>
        <brp:dienstIdentificatie>${parameters.dienstIdentificatie}</brp:dienstIdentificatie>
        <brp:zoekbereik>${parameters.zoekbereik}</brp:zoekbereik>
        <brp:peilmomentMaterieel>${parameters.peilmomentMaterieel}</brp:peilmomentMaterieel>
    </brp:parameters>
    <brp:zoekcriteria>
        [#list zoekcriteriaPersoon as zoekcriterium]
        <brp:zoekcriterium brp:objecttype="Zoekcriterium" brp:communicatieID="${zoekcriterium.communicatieID}">
            <brp:elementNaam>${zoekcriterium.elementnaam}</brp:elementNaam>
            <brp:waarde>${zoekcriterium.waarde}</brp:waarde>
            <brp:optie>${zoekcriterium.optie}</brp:optie>
        </brp:zoekcriterium>
        [/#list]
    </brp:zoekcriteria>
</brp:lvg_bvgZoekPersoonOpAdres>