<brp:bhg_vkzRegistreerKiesrecht xmlns:brp="http://www.bzk.nl/brp/brp0200">
    <brp:stuurgegevens brp:communicatieID="${stuurgegevens.communicatieID}">
        <brp:zendendePartij>${stuurgegevens.zendendePartij}</brp:zendendePartij>
        <brp:zendendeSysteem>${stuurgegevens.zendendeSysteem}</brp:zendendeSysteem>
        <brp:referentienummer>${stuurgegevens.referentienummer}</brp:referentienummer>
        <brp:tijdstipVerzending>${stuurgegevens.tijdstipVerzending}</brp:tijdstipVerzending>
    </brp:stuurgegevens>
    <brp:parameters brp:communicatieID="${parameters.communicatieID}">
        <brp:verwerkingswijze>${parameters.verwerkingswijze}</brp:verwerkingswijze>
        <!--Optional:-->
        <brp:bezienVanuitPersoon>${parameters.bezienVanuitPersoon}</brp:bezienVanuitPersoon>
    </brp:parameters>
    <!--You have a CHOICE of the next 2 items at this level-->
    <brp:wijzigingUitsluitingKiesrecht brp:communicatieID="${wijzigingUitsluitingKiesrecht.communicatieID}" brp:objecttype="AdministratieveHandeling">
        <brp:partijCode>${wijzigingUitsluitingKiesrecht.partijCode}</brp:partijCode>
        <!--Optional:-->
        <brp:toelichtingOntlening>${wijzigingUitsluitingKiesrecht.toelichtingOntlening}</brp:toelichtingOntlening>
        <!--Optional:-->
        <brp:gedeblokkeerdeMeldingen>
            <!--1 or more repetitions:-->
            [#list wijzigingUitsluitingKiesrecht.gedeblokkeerdeMeldingen as gedeblokkeerdeMelding]
            <brp:gedeblokkeerdeMelding brp:communicatieID="${gedeblokkeerdeMelding.communicatieID}" brp:objecttype="GedeblokkeerdeMelding" brp:referentieID="${gedeblokkeerdeMelding.referentieID}">
                <brp:regelCode>${gedeblokkeerdeMelding.regelCode}</brp:regelCode>
            </brp:gedeblokkeerdeMelding>
            [/#list]
        </brp:gedeblokkeerdeMeldingen>
        <!--Optional:-->
        <brp:bronnen>
            <!--1 or more repetitions:-->
            [#list wijzigingUitsluitingKiesrecht.bronnen as bron]
            <brp:bron brp:communicatieID="${bron.communicatieID}" brp:objecttype="AdministratieveHandelingBron">
                <!--Optional:-->
                <brp:document brp:communicatieID="${bron.document.communicatieID}" brp:objecttype="Document">
                    <brp:soortNaam>${bron.document.soortNaam}</brp:soortNaam>
                    <brp:identificatie>${bron.document.identificatie}</brp:identificatie>
                    <!--Optional:-->
                    <brp:aktenummer>${bron.document.aktenummer}</brp:aktenummer>
                    <brp:partijCode>${bron.document.partijCode}</brp:partijCode>
                </brp:document>
                <!--Optional:-->
                <brp:rechtsgrondCode>${bron.rechtsgrondCode}</brp:rechtsgrondCode>
                <!--Optional:-->
                <brp:rechtsgrondomschrijving>${bron.rechtsgrondomschrijving}</brp:rechtsgrondomschrijving>
            </brp:bron>
            [/#list]
        </brp:bronnen>
        <brp:acties>
            [#assign registratieUitsluitingKiesrecht = wijzigingUitsluitingKiesrecht.acties.registratieUitsluitingKiesrecht]
            <brp:registratieUitsluitingKiesrecht brp:communicatieID="${registratieUitsluitingKiesrecht.communicatieID}" brp:objecttype="Actie">
                <brp:datumAanvangGeldigheid>${registratieUitsluitingKiesrecht.datumAanvangGeldigheid}</brp:datumAanvangGeldigheid>
                <!--Optional:-->
                <brp:datumEindeGeldigheid>${registratieUitsluitingKiesrecht.datumEindeGeldigheid}</brp:datumEindeGeldigheid>
                <!--Optional:-->
                <brp:bronnen>
                    <!--1 or more repetitions:-->
                    [#list registratieUitsluitingKiesrecht.bronnen as bron]
                    <brp:bron brp:communicatieID="${bron.communicatieID}" brp:objecttype="ActieBron" brp:referentieID="${bron.referentieID}"/>
                    [/#list]
                </brp:bronnen>
                <brp:persoon brp:communicatieID="${registratieUitsluitingKiesrecht.persoon.communicatieID}" brp:objectSleutel="[@objectsleutel var=registratieUitsluitingKiesrecht.persoon.objectSleutel partij=stuurgegevens.zendendePartij/]" brp:objecttype="Persoon">
                    <brp:uitsluitingKiesrecht brp:communicatieID="${registratieUitsluitingKiesrecht.persoon.uitsluitingKiesrecht.communicatieID}">
                        <brp:indicatie>${registratieUitsluitingKiesrecht.persoon.uitsluitingKiesrecht.indicatie}</brp:indicatie>
                        <!--Optional:-->
                        <brp:datumVoorzienEinde>${registratieUitsluitingKiesrecht.persoon.uitsluitingKiesrecht.datumVoorzienEinde}</brp:datumVoorzienEinde>
                    </brp:uitsluitingKiesrecht>
                </brp:persoon>
            </brp:registratieUitsluitingKiesrecht>
        </brp:acties>
    </brp:wijzigingUitsluitingKiesrecht>
    <brp:wijzigingDeelnameEUVerkiezingen brp:communicatieID="${wijzigingDeelnameEUVerkiezingen.communicatieID}" brp:objecttype="AdministratieveHandeling">
        <brp:partijCode>${wijzigingDeelnameEUVerkiezingen.partijCode}</brp:partijCode>
        <!--Optional:-->
        <brp:toelichtingOntlening>${wijzigingDeelnameEUVerkiezingen.toelichtingOntlening}</brp:toelichtingOntlening>
        <!--Optional:-->
        <brp:gedeblokkeerdeMeldingen>
            <!--1 or more repetitions:-->
            [#list wijzigingDeelnameEUVerkiezingen.gedeblokkeerdeMeldingen as gedeblokkeerdeMelding]
            <brp:gedeblokkeerdeMelding brp:communicatieID="${gedeblokkeerdeMelding.communicatieID}" brp:objecttype="GedeblokkeerdeMelding" brp:referentieID="${gedeblokkeerdeMelding.referentieID}">
                <brp:regelCode>${gedeblokkeerdeMelding.regelCode}</brp:regelCode>
            </brp:gedeblokkeerdeMelding>
            [/#list]
        </brp:gedeblokkeerdeMeldingen>
        <!--Optional:-->
        <brp:bronnen>
            <!--1 or more repetitions:-->
            [#list wijzigingDeelnameEUVerkiezingen.bronnen as bron]
            <brp:bron brp:communicatieID="${bron.communicatieID}" brp:objecttype="AdministratieveHandelingBron">
                <!--Optional:-->
                <brp:document brp:communicatieID="${bron.document.communicatieID}" brp:objecttype="Document">
                    <brp:soortNaam>${bron.document.soortNaam}</brp:soortNaam>
                    <brp:identificatie>${bron.document.identificatie}</brp:identificatie>
                    <!--Optional:-->
                    <brp:aktenummer>${bron.document.aktenummer}</brp:aktenummer>
                    <brp:partijCode>${bron.document.partijCode}</brp:partijCode>
                </brp:document>
                <!--Optional:-->
                <brp:rechtsgrondCode>${bron.rechtsgrondCode}</brp:rechtsgrondCode>
                <!--Optional:-->
                <brp:rechtsgrondomschrijving>${bron.rechtsgrondomschrijving}</brp:rechtsgrondomschrijving>
            </brp:bron>
            [/#list]
        </brp:bronnen>
        <brp:acties>
            [#assign registratieDeelnameEUVerkiezingen = wijzigingDeelnameEUVerkiezingen.acties.registratieDeelnameEUVerkiezingen]
            <brp:registratieDeelnameEUVerkiezingen brp:communicatieID="${registratieDeelnameEUVerkiezingen.communicatieID}" brp:objecttype="Actie">
                <brp:datumAanvangGeldigheid>${registratieDeelnameEUVerkiezingen.datumAanvangGeldigheid}</brp:datumAanvangGeldigheid>
                <!--Optional:-->
                <brp:datumEindeGeldigheid>${registratieDeelnameEUVerkiezingen.datumEindeGeldigheid}</brp:datumEindeGeldigheid>
                <!--Optional:-->
                <brp:bronnen>
                    <!--1 or more repetitions:-->
                    [#list registratieDeelnameEUVerkiezingen.bronnen as bron]
                    <brp:bron brp:communicatieID="${bron.communicatieID}" brp:objecttype="ActieBron" brp:referentieID="${bron.referentieID}"/>
                    [/#list]
                </brp:bronnen>
                <brp:persoon brp:communicatieID="${registratieDeelnameEUVerkiezingen.persoon.communicatieID}" brp:objectSleutel="[@objectsleutel var=registratieDeelnameEUVerkiezingen.persoon.objectSleutel partij=stuurgegevens.zendendePartij/]" brp:objecttype="Persoon">
                    <brp:deelnameEUVerkiezingen brp:communicatieID="${wijzigingDeelnameEUVerkiezingen.acties.registratieDeelnameEUVerkiezingen.persoon.deelnameEUVerkiezingen.communicatieID}">
                        <brp:indicatieDeelname>${registratieDeelnameEUVerkiezingen.persoon.deelnameEUVerkiezingen.indicatieDeelname}</brp:indicatieDeelname>
                        <brp:datumAanleidingAanpassing>${registratieDeelnameEUVerkiezingen.persoon.deelnameEUVerkiezingen.datumAanleidingAanpassing}</brp:datumAanleidingAanpassing>
                        <!--Optional:-->
                        <brp:datumVoorzienEindeUitsluiting>${registratieDeelnameEUVerkiezingen.persoon.deelnameEUVerkiezingen.datumVoorzienEindeUitsluiting}</brp:datumVoorzienEindeUitsluiting>
                    </brp:deelnameEUVerkiezingen>
                </brp:persoon>
            </brp:registratieDeelnameEUVerkiezingen>
        </brp:acties>
    </brp:wijzigingDeelnameEUVerkiezingen>
</brp:bhg_vkzRegistreerKiesrecht>
