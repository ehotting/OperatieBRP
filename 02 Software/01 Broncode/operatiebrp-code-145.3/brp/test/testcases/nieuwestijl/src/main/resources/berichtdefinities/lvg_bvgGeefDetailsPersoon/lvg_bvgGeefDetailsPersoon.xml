<brp:lvg_bvgGeefDetailsPersoon xmlns:brp="http://www.bzk.nl/brp/brp0200">
    <brp:stuurgegevens brp:communicatieID="${stuurgegevens.communicatieID}">
        <brp:zendendePartij>${stuurgegevens.zendendePartij}</brp:zendendePartij>
        <brp:zendendeSysteem>${stuurgegevens.zendendeSysteem}</brp:zendendeSysteem>
        <brp:referentienummer>${stuurgegevens.referentienummer}</brp:referentienummer>
        <brp:tijdstipVerzending>${stuurgegevens.tijdstipVerzending}</brp:tijdstipVerzending>
    </brp:stuurgegevens>
    <brp:parameters brp:communicatieID="${parameters.communicatieID}">
        <brp:rolNaam>${parameters.rolNaam}</brp:rolNaam>
        <brp:leveringsautorisatieIdentificatie>${parameters.leveringsautorisatieIdentificatie}</brp:leveringsautorisatieIdentificatie>
        <brp:dienstIdentificatie>${parameters.dienstIdentificatie}</brp:dienstIdentificatie>
        <brp:peilmomentMaterieelResultaat>${parameters.peilmomentMaterieelResultaat}</brp:peilmomentMaterieelResultaat>
        <brp:peilmomentFormeelResultaat>${parameters.peilmomentFormeelResultaat}</brp:peilmomentFormeelResultaat>
        <brp:historievorm>${parameters.historievorm}</brp:historievorm>
    </brp:parameters>
    <brp:identificatiecriteria brp:communicatieID="03V">
        <brp:burgerservicenummer>${identificatiecriteria.burgerservicenummer}</brp:burgerservicenummer>
        <brp:administratienummer>${identificatiecriteria.administratienummer}</brp:administratienummer>
    </brp:identificatiecriteria>
    <!--Optional:-->
    <brp:scopeElementen brp:communicatieID="comid.scopeElementen">
        [#list scopeElementen as scopeElement]
        <brp:elementNaam>${scopeElement.elementnaam}</brp:elementNaam>
        [/#list]
    </brp:scopeElementen>
</brp:lvg_bvgGeefDetailsPersoon>
