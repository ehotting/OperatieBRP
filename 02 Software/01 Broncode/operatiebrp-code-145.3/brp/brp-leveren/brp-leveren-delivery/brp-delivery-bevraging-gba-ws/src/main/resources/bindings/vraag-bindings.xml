<jaxb:bindings version="2.0"
               xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <jaxb:bindings schemaLocation="../xsd/ws.xsd">
        <jaxb:bindings node="/xs:schema/xs:element[@name='vraag']">
            <jaxb:class name="VraagOperation"/>
        </jaxb:bindings>
        <jaxb:bindings node="/xs:schema/xs:element[@name='vraagPL']">
            <jaxb:class name="VraagPLOperation"/>
        </jaxb:bindings>
        <jaxb:bindings node="/xs:schema/xs:element[@name='vraag']//xs:element[@name='in0']">
            <jaxb:property name="vraag"/>
        </jaxb:bindings>
        <jaxb:bindings node="/xs:schema/xs:element[@name='vraagPL']//xs:element[@name='in0']">
            <jaxb:property name="vraagPL"/>
        </jaxb:bindings>
        <jaxb:schemaBindings>
            <jaxb:package name="nl.bzk.brp.delivery.bevraging.gba.ws.vraag"/>
        </jaxb:schemaBindings>
    </jaxb:bindings>
    <jaxb:bindings schemaLocation="../xsd/lo3.xsd">
        <jaxb:schemaBindings>
            <jaxb:package name="nl.bzk.brp.delivery.bevraging.gba.ws.lo3"/>
        </jaxb:schemaBindings>
    </jaxb:bindings>
</jaxb:bindings>
