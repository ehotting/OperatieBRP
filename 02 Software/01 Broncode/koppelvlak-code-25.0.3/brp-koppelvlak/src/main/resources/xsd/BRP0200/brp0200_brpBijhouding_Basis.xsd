	<!-- ... -->
	<!-- Alle Berichtelementen Bijhouding die voor meerdere modules een rol spelen. Specifieke vulling van objecten/groepen opgenomen in BRPxxxx_<BZM>.xsd's. -->
	<!-- ... -->
	<!-- Referenties naar Personen in berichten; indien alleen verwijzing naar Ingeschrevene (hoofdpersson) van toepassing is, dan volstaat opgave 'objectsleutel' -->
	<complexType name="Objecttype_Persoon_IdentificatieIngeschrevene_Bijhouding">
		<complexContent>
			<restriction base="brp:Objecttype_Persoon">
				<sequence/>
				<attribute ref="brp:objecttype" use="required" fixed="Persoon"/>
				<attribute ref="brp:verwerkingssoort" use="prohibited"/>
				<attribute ref="brp:objectSleutel" use="required"/>
				<attribute ref="brp:voorkomenSleutel" use="prohibited" />
				<attribute ref="brp:referentieID" use="prohibited"/>
				<attribute ref="brp:communicatieID" use="required"/>
			</restriction>
		</complexContent>
	</complexType>
	<!-- Referenties naar Personen in berichten: -->
	<!-- 1) Igv gerelateerden (ouder/kind/partner) verwijzing naar een 'Ingeschrevene' mogelijk (via objectsleutel); -->
	<!-- 2) Igv gerelateerde als 'Pseudo-persoon' de vulling van de identificerende groepen --> 
	<complexType name="Objecttype_Persoon_Identificatie_Bijhouding">
		<complexContent>
			<restriction base="brp:Objecttype_Persoon">
				<sequence> 
					<element name="identificatienummers" type="brp:Groep_PersoonIdentificatienummersPseudopersoon_Bijhouding" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="samengesteldeNaam" type="brp:Groep_PersoonSamengesteldeNaamPseudopersoon_Bijhouding" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="geboorte" type="brp:Groep_PersoonGeboortePseudopersoon_Bijhouding" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="geslachtsaanduiding" type="brp:Groep_PersoonGeslachtsaanduidingPseudopersoon_Bijhouding" nillable="false" minOccurs="0" maxOccurs="1"/>
				</sequence>
				<attribute ref="brp:objecttype" use="required" fixed="Persoon"/>
				<attribute ref="brp:verwerkingssoort" use="prohibited"/>
				<attribute ref="brp:objectSleutel" use="optional"/>
				<attribute ref="brp:voorkomenSleutel" use="prohibited" />
				<attribute ref="brp:referentieID" use="prohibited"/>
				<attribute ref="brp:communicatieID" use="required"/>
			</restriction>
		</complexContent>
	</complexType>
